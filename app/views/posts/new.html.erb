<div class="container">
  <div class="row center">
    <h1>Add New Texas Image Post</h1>
  </div>
  <div class="row">
    <div class="col-md-6 col-md-offset-3">
      <%= bootstrap_form_for(@post) do |f| %>
        <%= f.text_field :title %>
        <%= f.text_field :description %>
        <%= f.file_field :post_image %>
        <%= f.submit "Submit Post" %>
      <% end %>
    </div>
  </div>
</div>
